{
    "name": "tahajaiti/jwt",
    "description": "A laravel package for JWT Authentication",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kyojin\\JWT\\": "src/"
        }
    },
    "authors": [
        {
            "name": "taha jaiti",
            "email": "kyojin.rar@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "illuminate/auth": "^10|^11|^12",
        "illuminate/contracts": "^10|^11|^12",
        "illuminate/http": "^10|^11|^12",
        "illuminate/support": "^10|^11|^12"
    },
    "require-dev": {
        "illuminate/support": "^12.4",
        "illuminate/console": "^12.4",
        "illuminate/contracts": "^12.4",
        "illuminate/http": "^12.4",
        "laravel/framework": "^12.0",
        "illuminate/routing": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kyojin\\JWT\\Providers\\JWTServiceProvider"
            ],
            "aliases": {
                "JWT": "Kyojin\\JWT\\Facades\\JWT"
            }
        }
    }
}